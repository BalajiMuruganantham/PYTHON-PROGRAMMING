
*******************************************************************************/
#include <stdio.h>

int main()
{
    char a[20];
    int x=0;
    scanf("%s",a);
    n=strlen(a);
    for(i=1;i<n;++i)
    {
        if(a[i-1]!=a[i])
        {
            printf("%d,%d",&a[i-1],&x);
        
        else
        
            x=x+1;
        }
    }
}
